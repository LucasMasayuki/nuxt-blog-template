{
    "processors": [
        "stylelint-processor-html"
    ],
    "extends": "stylelint-config-standard",
    "customSyntax": "postcss-scss",
    "files": [
        "./**/*.vue",
        "./**/*.scss",
        "./**/*.css"
    ],
    "rules": {
        "indentation": 4,
        "no-empty-first-line": null,
        "no-empty-source": null
    }
}